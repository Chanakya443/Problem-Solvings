class Solution {
    public int findPeakElement(int[] nums) {
       int min=-1000;
        int index=0;
       for(int i=0;i<nums.length;i++)
       {
           if(min<nums[i])
           {
               min=nums[i];
               index=i;
               
           }
       }
        
        return index;
    }
}
